   "outputs": [],
   "source": [
    "from numpy import sin, linspace\n",
    "import plotly.express as px\n",
    "\n",
    "def f(x):\n",
    "    return 2 * sin(x**2 - 5)\n",
    "\n",
    "a, b = -7, 7\n",
    "n_points = 1000\n",
    "x = linspace(a, b, n_points)\n",
    "\n",
    "fig = px.line(x=x, y=f(x))\n",
    "fig.show()\n"
   ]
  }
